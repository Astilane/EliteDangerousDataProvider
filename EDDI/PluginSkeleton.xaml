<UserControl x:Class="EDDI.PluginSkeleton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EDDI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <DockPanel x:Name="panel" LastChildFill="True" Background="#FFE5E5E5">
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" Margin="10">
            <CheckBox x:Name="pluginenabled" Margin="10" Checked="pluginenabled_Checked" Unchecked="pluginenabled_Unchecked"></CheckBox>
            <TextBlock Margin="10">Plugin enabled</TextBlock>
        </StackPanel>
    </DockPanel>
</UserControl>
